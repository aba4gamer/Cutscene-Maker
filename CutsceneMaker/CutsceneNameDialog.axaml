<Window xmlns="https://github.com/avaloniaui"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
		xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
		mc:Ignorable="d" Width="330" Height="125"
		x:Class="CutsceneMaker.CutsceneNameDialog"
		x:Name="CutsceneNameDialogWindow"
		Background="#3a3a3a"
		WindowStartupLocation="CenterOwner"
		CanResize="False"
		Focusable="True">
	<StackPanel Margin="10" Spacing="8">
		<TextBlock Name="Body" Height="20"/>
		<TextBox Name="NameBox" Watermark="Ex: DemoMyCoolCutscene"/>
		<StackPanel Orientation="Horizontal" HorizontalAlignment="Right" Spacing="6">
			<Button Name="CancelButton" Content="Cancel" Click="OnCancel" />
			<Button Name="SubmitButton" Content="Submit" Click="OnSubmit" />
		</StackPanel>
	</StackPanel>
</Window>
