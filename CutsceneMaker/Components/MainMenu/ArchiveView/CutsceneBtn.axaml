<UserControl xmlns="https://github.com/avaloniaui"
			 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
			 xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
			 mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
			 x:Class="CutsceneMakerUI.CutsceneBtn">
	<Button Name="MainBtn" HorizontalAlignment="Stretch" Click="OnClickCutscene">
		<StackPanel Orientation="Horizontal" VerticalAlignment="Center">
			<Image Source="avares://CutsceneMaker/Assets/placeholder.png" Width="14" Height="14" />
			<Label Name="CutsceneNameLabel" FontSize="12"></Label>
		</StackPanel>
	</Button>
</UserControl>
