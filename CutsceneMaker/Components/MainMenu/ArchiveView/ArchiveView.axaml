<UserControl xmlns="https://github.com/avaloniaui"
			 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
			 xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
			 mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
			 x:Class="CutsceneMakerUI.ArchiveView">
	<DockPanel>
		<Grid ColumnDefinitions="0.5*, 4, 1.8*">
			<Grid Grid.Column="0" Name="Sidebar" ColumnDefinitions="1*" Background="#323232" HorizontalAlignment="Stretch" />
			<GridSplitter Grid.Column="1" Background="Black" ResizeDirection="Columns"/>
			<Grid Grid.Column="2" Name="Main" ColumnDefinitions="1*" HorizontalAlignment="Stretch" />
		</Grid>
	</DockPanel>
</UserControl>
