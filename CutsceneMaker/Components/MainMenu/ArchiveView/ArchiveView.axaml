<UserControl xmlns="https://github.com/avaloniaui"
			 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
			 xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
			 mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
			 x:Class="CutsceneMakerUI.ArchiveView">
	<DockPanel>
		<SplitView DisplayMode="Inline" PanePlacement="Left" OpenPaneLength="150" IsPaneOpen="True">
			<SplitView.Pane>
				<Grid Name="Sidebar" ColumnDefinitions="1*" Background="#323232" HorizontalAlignment="Stretch"></Grid>
			</SplitView.Pane>
			<SplitView.Content>
				<Grid Name="Main" ColumnDefinitions="1*" HorizontalAlignment="Stretch"></Grid>
			</SplitView.Content>
		</SplitView>
	</DockPanel>
</UserControl>
