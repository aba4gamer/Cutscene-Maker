<UserControl xmlns="https://github.com/avaloniaui"
			 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
			 xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
			 mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
			 x:Class="CutsceneMakerUI.CutsceneView">
	<Grid RowDefinitions="1.6*, 4, 1*">
		<Grid Grid.Row="0" Name="Main" ColumnDefinitions="1*" HorizontalAlignment="Stretch" />
		<GridSplitter Grid.Row="1" Background="Black" ResizeDirection="Rows"/>
		<DockPanel Grid.Row="2" Name="Timeline" Background="#3a3a3a" VerticalAlignment="Stretch" />
	</Grid>
</UserControl>
