<UserControl xmlns="https://github.com/avaloniaui"
			 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
			 xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
			 xmlns:views="using:CutsceneMakerUI"
			 mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
			 x:Class="CutsceneMakerUI.CutsceneView">
	<SplitView Background="#3a3a3a" DisplayMode="Inline" PanePlacement="Bottom" OpenPaneLength="200" IsPaneOpen="True">
		<SplitView.Content>
			<Grid Name="Main" ColumnDefinitions="1*" HorizontalAlignment="Stretch"></Grid>
		</SplitView.Content>
		<SplitView.Pane>
			<StackPanel Name="Timeline" Background="#3a3a3a" VerticalAlignment="Bottom"></StackPanel>
		</SplitView.Pane>
	</SplitView>
</UserControl>
