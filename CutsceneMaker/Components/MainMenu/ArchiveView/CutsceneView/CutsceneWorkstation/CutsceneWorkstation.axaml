<UserControl xmlns="https://github.com/avaloniaui"
			 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
			 xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
			 mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
			 x:Class="CutsceneMakerUI.CutsceneWorkstation">
	<TabControl HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
		<TabItem Name="PartTab" Header="Main Part" FontSize="15">
			<ScrollViewer>
				<StackPanel Name="PartTabContent"></StackPanel>
			</ScrollViewer>
		</TabItem>
		<TabItem Header="Player" FontSize="15">
			<ScrollViewer>
				<StackPanel Name="PlayerTab"></StackPanel>
			</ScrollViewer>
		</TabItem>
		<TabItem Header="Action" FontSize="15">
			<ScrollViewer>
				<StackPanel Name="ActionTab"></StackPanel>
			</ScrollViewer>
		</TabItem>
		<TabItem Header="Camera" FontSize="15">
			<ScrollViewer>
				<StackPanel Name="CameraTab"></StackPanel>
			</ScrollViewer>
		</TabItem>
		<TabItem Header="Sound" FontSize="15">
			<ScrollViewer>
				<StackPanel Name="SoundTab"></StackPanel>
			</ScrollViewer>
		</TabItem>
		<TabItem Header="Wipe" FontSize="15">
			<ScrollViewer>
				<StackPanel Name="WipeTab"></StackPanel>
			</ScrollViewer>
		</TabItem>
	</TabControl>
</UserControl>
