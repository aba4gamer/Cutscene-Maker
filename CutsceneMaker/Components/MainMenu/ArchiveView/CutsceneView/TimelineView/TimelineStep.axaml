<UserControl xmlns="https://github.com/avaloniaui"
			 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
			 xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
			 mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
			 x:Class="CutsceneMakerUI.TimelineStep">
	<DockPanel Name="Main" HorizontalAlignment="Left" VerticalAlignment="Stretch">
		<Border BorderThickness="0, 0, 1, 0" BorderBrush="#8f8f8f" VerticalAlignment="Stretch">
			<Label Name="StepCountLabel" FontSize="8" Foreground="#8f8f8f" HorizontalAlignment="Right" VerticalAlignment="Center"></Label>
		</Border>
	</DockPanel>
</UserControl>
