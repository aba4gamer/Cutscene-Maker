<UserControl xmlns="https://github.com/avaloniaui"
			 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
			 xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
			 mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
			 x:Class="CutsceneMakerUI.TimelineDeco">
	<Border BorderThickness="0, 0, 1, 0" BorderBrush="White">
		<Grid RowDefinitions="1*, 4*, 1*, 4*" HorizontalAlignment="Left">
			<DockPanel Grid.Row="0" Width="80" Background="#112"></DockPanel>
			<Border Grid.Row="1" BorderThickness="0, 0, 0, 1" BorderBrush="White">
				<DockPanel Background="#223" HorizontalAlignment="Stretch">
					<Grid ColumnDefinitions="1*" Width="80" Height="60" HorizontalAlignment="Left" VerticalAlignment="Center">
						<StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
							<Label FontSize="8">Sequence</Label>
						</StackPanel>
					</Grid>
				</DockPanel>
			</Border>
			<DockPanel Grid.Row="2" Width="80" Background="#112"></DockPanel>
			<DockPanel Grid.Row="3" Background="#191929" HorizontalAlignment="Stretch">
				<Grid ColumnDefinitions="1*" Width="80" Height="60" HorizontalAlignment="Left" VerticalAlignment="Center">
					<StackPanel HorizontalAlignment="Center" VerticalAlignment="Center" Spacing="1">
						<Label FontSize="8" HorizontalAlignment="Center">Sub</Label>
						<Label FontSize="8" HorizontalAlignment="Center">Sequence</Label>
					</StackPanel>
				</Grid>
			</DockPanel>
		</Grid>
	</Border>
</UserControl>
