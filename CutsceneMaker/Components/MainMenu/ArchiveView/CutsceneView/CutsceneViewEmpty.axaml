<UserControl xmlns="https://github.com/avaloniaui"
			 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
			 xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
			 mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
			 x:Class="CutsceneMakerUI.CutsceneViewEmpty">
	<StackPanel HorizontalAlignment="Center" VerticalAlignment="Center" Spacing="5">
		<TextBlock HorizontalAlignment="Center" Foreground="#8f8f8f" Text="No cutscene selected!" />
		<TextBlock HorizontalAlignment="Center" Foreground="#8f8f8f" Text="" />
		<TextBlock HorizontalAlignment="Center" Foreground="#8f8f8f" Text="Please select a cutscene to start editing." />
	</StackPanel>
</UserControl>
